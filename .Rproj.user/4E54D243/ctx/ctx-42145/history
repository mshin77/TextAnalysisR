capture_all_output <- function(expr) {
message_lines <- character(0)
# Capture everything printed to the normal output stream
output_lines <- withCallingHandlers(
expr = capture.output({
force(expr)
}, type = "output"),
# Capture and store messages into message_lines
message = function(m) {
message_lines <<- c(message_lines, m$message)
invokeRestart("muffleMessage")
}
)
c(output_lines, message_lines)
}
preprocess_texts <- function(united_tbl,
text_field = "united_texts",
min_char = 2,
remove_punct = TRUE,
remove_symbols = TRUE,
remove_numbers = TRUE,
remove_url = TRUE,
remove_separators = TRUE,
split_hyphens = TRUE,
split_tags = TRUE,
keep_acronyms = FALSE,
include_stopwords = TRUE,
verbose = FALSE) {
# Capture all output from these operations
verbose_output <- capture_all_output({
if (verbose) message("Creating corpus...")
corp <- corpus(united_tbl[[text_field]])
if (verbose) message("Tokenizing text...")
toks <- tokens(
corp,
what = "word",
remove_punct = remove_punct,
remove_symbols = remove_symbols,
remove_numbers = remove_numbers,
remove_url = remove_url,
remove_separators = remove_separators,
split_hyphens = split_hyphens,
split_tags = split_tags,
include_docvars = TRUE,
padding = FALSE,
verbose = verbose
)
if (verbose) message("Converting tokens to lowercase...")
toks_lower <- tokens_tolower(toks, keep_acronyms = keep_acronyms)
# Optional stopwords removal
all_stops <- if (include_stopwords) stopwords("en") else NULL
if (verbose) message("Removing stopwords...")
toks_no_stops <- tokens_remove(toks_lower, all_stops, verbose = verbose, padding = TRUE)
if (verbose) message("Selecting tokens with minimum character length...")
toks_clean <- tokens_select(toks_no_stops, min_nchar = min_char, verbose = verbose)
if (verbose) message("Finished preprocessing.")
})
list(toks_clean = toks_clean, verbose_output = verbose_output)
}
detect_multi_word_expressions <- function(toks_clean) {
tstat <- quanteda.textstats::textstat_collocations(toks_clean, size = 2:3, min_count = 2)
tstat_collocation <- tstat$collocation
return(tstat_collocation)
}
